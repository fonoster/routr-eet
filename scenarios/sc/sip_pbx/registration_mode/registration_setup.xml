<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE scenario SYSTEM "sipp.dtd">

<scenario name="SC-IT.Conf.1.1.1: Registration Setup">

  <recv request="REGISTER" crlf="true">
    <action>
      <ereg regexp="^.*sp.lab.com.*$" search_in="msg" case_indep="true" check_it="true" assign_to="pass"/>
      <ereg regexp="^.*gin.*$" search_in="hdr" header="Proxy-Require" case_indep="true" check_it="true" assign_to="pass"/>
      <ereg regexp="^.*gin.*$" search_in="hdr" header="Require" case_indep="true" check_it="true" assign_to="pass"/>
      <ereg regexp="^.*path.*$" search_in="hdr" header="Supported" case_indep="true" check_it="true" assign_to="pass"/>
      <ereg regexp="^.*pbx-1@sp.lab.com.*$" search_in="hdr" header="From" case_indep="true" check_it="true" assign_to="pass"/>
      <ereg regexp="^.*pbx-1@sp.lab.com.*$" search_in="hdr" header="To" case_indep="true" check_it="true" assign_to="pass"/>
      <!--TODO: Ensure it doen't have username or @ symbol-->
      <ereg regexp="^.*sip:.*;bnc.*$" search_in="hdr" header="Contact" case_indep="true" check_it="true" assign_to="pass"/>
      <test assign_to="pass" variable="pass" compare="equal" value="1" />
    </action>
  </recv>

</scenario>
